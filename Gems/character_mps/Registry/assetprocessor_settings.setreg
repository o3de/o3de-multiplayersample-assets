{
    "Amazon": {
        "AssetProcessor": {
            "Settings": {
                "ScanFolder character_mps/Assets": {
                    "watch": "@GEMROOT:character_mps@/Assets",
                    "recursive": 1,
                    "order": 101
                },
                "ScanFolder character_mps/Registry": {
                    "watch": "@GEMROOT:character_mps@/Registry",
                    "recursive": 1,
                    "order": 102
                },
			    "Exclude":{
				    "Exclude .src source folders": {
					    "glob": "*/.src/*"
					}
				},
                "Exclude":{
                    "Exclude Maya .mayaSwatches folders": {
                        "glob": "*/.mayaSwatches/*"
                    }
                },
                "Exclude":{
                    "Exclude Maya .mb files": {
                        "glob": "*.mb"
                    }
                }
            }
        }
    }
}
